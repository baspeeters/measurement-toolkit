{
  "name": "unit-converter",
  "version": "0.0.9",
  "description": "Object Oriented Unit Converter using TypeScript",
  "scripts": {
    "lint": "$(npm bin)/tslint -c tslint.json src/**/*.ts ./test/**/*.ts",
    "test": "$(npm bin)/mocha --require ts-node/register --recursive ./**/*.spec.ts",
    "test:coverage": "$(npm bin)/nyc --reporter=text --reporter=text-summary npm run test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/baspeeters/unit-converter.git"
  },
  "keywords": [
    "converter",
    "conversion",
    "weight",
    "mass",
    "metric",
    "imperial",
    "kilograms"
  ],
  "author": "Bas Peeters <bas@peete.rs>",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/baspeeters/unit-converter/issues"
  },
  "homepage": "https://github.com/baspeeters/unit-converter#readme",
  "devDependencies": {
    "@types/chai": "^4.2.6",
    "@types/mocha": "^5.2.7",
    "chai": "^4.2.0",
    "istanbul": "^0.4.5",
    "mocha": "^6.2.2",
    "nyc": "^14.1.1",
    "ts-node": "^8.5.4",
    "tslint": "^5.20.1",
    "typescript": "^3.7.3"
  },
  "dependencies": {}
}
